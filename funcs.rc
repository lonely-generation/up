}


wiptestdebug(){
    goup task task -d ./tests/functests -t wip -i dev -v vvvvv
}

wiptestvalidate(){
    goup validate task task -d ./tests/functests -t wip -i dev -v vvvvv
}

uplist(){
goup list task
}

