goup(){
    go run app/up/*.go $@
}

wiptest(){
    goup task task -d ./tests/functests -t wip -i dev --configdir=./tests/functests
}

wipfunctest(){
    gotest ./tests/functests/wip_test.go -v
}

wiptestvvvv(){
    goup task task -d ./tests/functests -t wip -i dev -v vvvv --configdir=./tests/functests
}


wiptestvv(){
    goup task task -d ./tests/functests -t wip -i dev -v vv --configdir=./tests/functests
}


wiptestvvv(){
    goup task task -d ./tests/functests -t wip -i dev -v vvv --configdir=./tests/functests
}

wiptestvvvvv(){
