}

man_funclist(){
cat funclist |fzf
}

start_nix_docker(){
docker run -it -v `pwd`:/data nixos/nix /bin/sh
}

start_up_dev_docker(){
docker run -it -v `pwd`:/up -v ${HUGO_DOC_DIR}:/updocs cmgolang /bin/bash
}

